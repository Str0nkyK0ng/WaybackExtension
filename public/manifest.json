{
  "name": "Tab Substitutor",
  "description": "Base Level Extension",
  "version": "2.0",
  "manifest_version": 3,
  "host_permissions": ["<all_urls>"],
  "permissions": ["tabs", "scripting", "activeTab", "notifications"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Click to show an alert"
  }
}
